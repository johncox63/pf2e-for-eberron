{"name":"Darkleaf branded mail","type":"armor","img":"systems/pf2e/icons/equipment/armor/leather-armor.webp","effects":[],"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.4tIVTg9wj56RrveA"},"cf":null},"system":{"description":{"value":"<div class=\"flex-even column\" data-markdown=\"1\">\n<div class=\"content\" data-markdown=\"1\">\n<div class=\"flex-even column\" data-markdown=\"1\">\n<div class=\"content\" data-markdown=\"1\">\n<p class=\"hang\">Similar to leafweave armour, darkleaf armour is made of carefully cured and beautifully carved pieces of wood, supplemented by treated <span style=\"font-family:var(--font-primary);font-size:var(--font-size-14)\">leaves. This special process enables the many primal practitioners of the world a sturdy, yet entirely natural armour.</span></p>\n</div>\n</div>\n</div>\n</div>"},"source":{"value":"PF2E for Eberron"},"traits":{"value":["flexible","primal"],"rarity":"common","custom":"","otherTags":[]},"rules":[],"slug":"leather-armor","schema":{"version":0.812,"lastMigration":{"datetime":"2023-01-06T12:50:30.314-05:00","version":{"schema":0.769,"foundry":"10.291","system":"4.6.3"}}},"quantity":1,"baseItem":"leather-armor","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"2"},"equippedBulk":{"value":"1"},"price":{"value":{"gp":10}},"equipped":{"carryType":"worn","invested":null},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":null},"preciousMaterialGrade":{"value":null},"size":"med","identification":{"status":"identified","unidentified":{"name":"Unusual Leather Armor","img":"systems/pf2e/icons/unidentified_item_icons/armor.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"wornarmor"},"level":{"value":0},"armor":{"value":3},"category":"medium","group":"composite","strength":{"value":14},"dex":{"value":2},"check":{"value":-2},"speed":{"value":-5},"potencyRune":{"value":0},"resiliencyRune":{"value":""},"propertyRune1":{"val