ger":{"value":""},"deathNote":false},"_stats":{"systemId":"pf2e","systemVersion":"4.6.3","coreVersion":"10.291","createdTime":1673027630193,"modifiedTime":1673030779288,"lastModifiedBy":"knTUi1bTspduSmIG"},"folder":null,"sort":0,"ownership":{"default":0,"knTUi1bTspduSmIG":3}}
